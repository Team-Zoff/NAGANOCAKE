<div class="continer-fluid">
   <div class="row form-group m-md-5">
     <div class="col-2 text-center">
	   <div class="text-light bg-info">注文履歴一覧</div>
     </div>
    </div>
<div class="continer-fluid">
  <div class="row justify-content-center">
    <table class="table col-6">
    <thead>
		<tr>
			<th scope="col">購入日時</th>
			<th>購入者</th>
			<th>注文個数</th>
			<th>注文ステータス</th>
		</tr>
	</thead>
	<tbody>
			<% @orders.each do |order| %>
		<tr>
			<td><%#= link_to order.created_at, admins_orders_show_path注文履歴詳細へとぶ %></td>
			<td><%#= order.member_id.last_name, order.member_id.first_name %></td>
			<td><%#= order.注文詳細モデルから引っ張る %></td>
			<td><%= order.order_status %></td>
		</tr>
		<% end %>
	</tbody>
</table>
</div>
</div>
</div>

